@model IEnumerable<LearningSystem.ViewModels.Study.AllSectionsViewModel>

@{
    ViewBag.Title = "Sections";
}

<h2>Choose a section for the  test</h2>

<a href="#">Add question</a>

@foreach (var sec in Model)
{
    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-4">
            <a href="/Test/Questions/@sec.Id">@sec.Title</a>
        </div>
    </div>

}
